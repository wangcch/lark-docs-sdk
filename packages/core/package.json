{
  "name": "lark-docs-sdk",
  "version": "1.0.0",
  "type": "module",
  "description": "Core library for Lark Docs Component",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "lark",
    "feishu",
    "docs",
    "component"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.7.2",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4"
  }
}
